ageNewsContainer,\n  MainPageDivision\n} from \"./styles\"\n\n\nconst MainPage = () => {\n  const [allNews, setAllNews] = useState([])\n  const history = useHistory()\n\n  const headers = { 'Access-Control-Allow-Origin': '*' }\n\n\n  const getNewsData = () => {\n    axios.get(`https://tech-archive-project.herokuapp.com/news/`,\n      headers\n    )\n      .then(resp => {\n        setAllNews(resp.data.data)\n\n      })\n      .catch((error) => { // erro no request\n        console.log(error)\n      })\n  }\n\n\n\n  const GoToNewsPage = (index) => {\n    history.push(`/newsId/${index + 1}`)\n  }\n\n\n  useEffect(() => {\n    getNewsData()\n    console.log(allNews)\n  }, [])\n\n\n\n\n  return (\n    <div>\n      {window.innerWidth > 640 ? <MainPageContainer>\n        <MainPageAdvertising src={propaganda} />\n\n        <MainPageNewsContainer>\n          <MainPageNews>\n            {allNews.map((news, index) => (\n              <NewsMainContainer key={index} onClick={() => GoToNewsPage(index)}>\n                <NewsImageContainer></NewsImageContainer>\n                <NewsTextContainer>\n                  <NewsTextTitle>{news.title}</NewsTextTitle>\n                  <NewsTextAuthorDate>Rafael,{news.create_at}</NewsTextAuthorDate>\n                  <NewsTextTheme>Tecnologia</NewsTextTheme>\n                </NewsTextContainer>\n              </NewsMainContainer>))}\n          </MainPageNews>\n          <MainPageDivision></MainPageDivision>\n          <MainPageNews>\n            {allNews.map((news, index) => (\n              <NewsMainContainer key={index} onClick={() => GoToNewsPage()}>\n                <NewsImageContainer></NewsImageContainer>\n                <NewsTextContainer>\n                  <NewsTextTitle>{news.title}</NewsTextTitle>\n                  <NewsTextAuthorDate>Rafael,{news.create_at}</NewsTextAuthorDate>\n                  <NewsTextTheme>Tecnologia</NewsTextTheme>\n                </NewsTextContainer>\n              </NewsMainContainer>))}\n          </MainPageNews>\n        </MainPageNewsContainer>\n        <MainPageAdvertising src={propaganda} />\n      </MainPageContainer> :\n        <MainPageContainer>\n          <MainPageNews>\n            <NewsComponent />\n            <NewsComponent />\n            <NewsComponent />\n          </MainPageNews>\n        </MainPageContainer>}\n    </div>\n\n  )\n}\n\nexport default MainPage","/home/rafael/q3/q3-capstone/front-end/tech-archive/src/pages/sign-up/index.js",[],"/home/rafael/q3/q3-capstone/front-end/tech-archive/src/components/newsform-layout/index.js",[],"/home/rafael/q3/q3-capstone/front-end/tech-archive/src/components/newsComponent/styles.js",[],"/home/rafael/q3/q3-capstone/front-end/tech-archive/src/components/layout-signup/index.js",[],"/home/rafael/q3/q3-capstone/front-end/tech-archive/src/routes/routes.jsx",[],{"ruleId":"123","replacedBy":"124"},{"ruleId":"125","replacedBy":"126"},{"ruleId":"123","replacedBy":"127"},{"ruleId":"125","replacedBy":"128"},{"ruleId":"123","replacedBy":"129"},{"ruleId":"125","replacedBy":"130"},{"ruleId":"123","replacedBy":"131"},{"ruleId":"125","replacedBy":"132"},{"ruleId":"123","replacedBy":"133"},{"ruleId":"125","replacedBy":"134"},{"ruleId":"135","severity":1,"message":"136","line":3,"column":22,"nodeType":"137","messageId":"138","endLine":3,"endColumn":31},"no-native-reassign",["139"],"no-negated-in-lhs",["140"],["139"],["140"],["139"],["140"],["139"],["140"],["139"],["140"],"no-unused-vars","'useParams' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]